<refentry xmlns="http://docbook.org/ns/docbook"
    xmlns:src="http://nwalsh.com/xmlns/litprog/fragment"
    xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
    xmlns:xlink="http://www.w3.org/1999/xlink"
    version="5.0" xml:id="styles.builtin.ignore">
    <refmeta>
        <refentrytitle>styles.builtin.ignore</refentrytitle>
        <refmiscinfo role="type">boolean</refmiscinfo>
    </refmeta>
    <refnamediv>
        <refname>styles.builtin.ignore</refname>
        <refpurpose>Don't embed links to the builtin css resources when generating output</refpurpose>
    </refnamediv>
    
    <refsynopsisdiv>
        <src:fragment xml:id='styles.builtin.ignore.frag'>
            <xsl:param name="styles.builtin.ignore"
                select="'0'"/>
        </src:fragment>
    </refsynopsisdiv>
    
    <refsect1><title>Description</title>
        
        <para>Disables embedding links to ancillary styles like prism
        </para>
        
    </refsect1>
</refentry>
